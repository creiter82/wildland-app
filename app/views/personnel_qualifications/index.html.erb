<p id="notice"><%= notice %></p>

<h1>Listing Personnel Qualifications</h1>

<table>
  <thead>
    <tr>
      <th>Personnel</th>
      <th>Qualification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personnel_qualifications.each do |personnel_qualification| %>
      <tr>
        <td><%= personnel_qualification.personnel_id %></td>
        <td><%= personnel_qualification.qualification_id %></td>
        <td><%= link_to 'Show', personnel_qualification %></td>
        <td><%= link_to 'Edit', edit_personnel_qualification_path(personnel_qualification) %></td>
        <td><%= link_to 'Destroy', personnel_qualification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Personnel qualification', new_personnel_qualification_path %>
